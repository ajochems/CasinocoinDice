<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="initializingTF">
    <default-activity>initBlockChain</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>chainDownloadProgress</managed-bean-name>
      <managed-bean-class>org.casinocoin.dice.beans.ChainDownloadProgressIndicator</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>InitBlockChain</managed-bean-name>
      <managed-bean-class>org.casinocoin.dice.beans.InitBlockChain</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="initBlockChain">
      <page>/initializing/initBlockChain.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
